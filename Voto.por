programa {
  funcao inicio() {
    inteiro idade

    escreva("Informe sua idade: ")
    leia(idade)

    se(idade>=70){escreva("Voto não obrigatório!")}
    senao se(idade>=18){escreva("Voto obrigatório!")}
    senao se(idade>=16){escreva("Voto Facultativo!")}
    senao se(idade<16){escreva("Voto não obrigatório!")}












  }
}
